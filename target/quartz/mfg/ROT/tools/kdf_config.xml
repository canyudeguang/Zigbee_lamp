<kdf_descriptor>
<!--
    ***kdf setting to generate password***
    op_code:        operation code 
                        opcode 7: RoT activation 
                        opcode 8: RoT revocation
                        opcode 10: Product Wrapped Key
    oem_id:         oem id
    model_id:       model id
    mid:            machine id
                        1: M4
    otp_encryp_key: otp encryp key
    dbg_enable:     debug flag
                        0:disble, 1:enable
    sw_input:       sw input
-->
    <op_code>0x8</op_code>
    <oem_id>0x6FB2</oem_id>
    <mid>0x01</mid>
    <model_id>0xF350</model_id>
    <otp_encryp_key>3c441d5a0ac5ec24fb7b0ded3ddcc250</otp_encryp_key>
    <dbg_enable>0x1</dbg_enable>
    <sw_input>00000000000000000000000000000001</sw_input>
</kdf_descriptor>

      

